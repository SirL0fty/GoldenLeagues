{
  "tabWidth": 4,
  "printWidth": 120,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "always",
  "trailingComma": "all",
  "overrides": [
    {
      "files": ["*.css"],
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": ["*.html"],
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": ["*.py"],
      "options": {
        "singleQuote": false,
        "trailingComma": "none"
      }
    }
  ]
}
